<?xml version="1.0" encoding="utf-8"?>
<TableLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_width="match_parent"
  android:layout_height="match_parent"
  android:background="@drawable/stars"
  android:text="Enter the Code!">
    <TableRow android:id="@+id/tableRow1" android:layout_width="wrap_content" android:layout_height="wrap_content">
        <TextView android:typeface="monospace" android:textSize="20dip" android:layout_width="wrap_content" android:id="@+id/thetimer" android:layout_height="wrap_content" android:text="TextView"></TextView>
    </TableRow>
    <TextView android:typeface="monospace" android:textSize="20dip" android:layout_height="wrap_content" android:id="@+id/CodeText" android:layout_width="wrap_content" android:text="Code: "></TextView>
    <TextView android:typeface="monospace" android:textSize="20dip" android:layout_height="wrap_content" android:id="@+id/YourText" android:text="TextView" android:layout_width="wrap_content"></TextView>
    <TableRow android:id="@+id/tableRow2" android:layout_width="wrap_content" android:layout_height="wrap_content">
        <Button
    			android:background="@drawable/rounded_button1" android:typeface="monospace" 
    			android:text="7"
    			android:textSize="70dp"
    			android:layout_width="0dip" 
    			android:layout_height="100dip" 
    			android:padding="10dip" 
    			android:layout_weight="1" 
    			android:layout_margin="4dip"
    			android:gravity="center" android:id="@+id/button7">
    		</Button>
    		<Button
    			android:background="@drawable/rounded_button1" android:typeface="monospace" 
    		    android:text="8"
    			android:layout_width="0dip" 
    			android:textSize="70dp"
    			android:layout_height="100dip" 
    			android:padding="10dip" 
    			android:layout_weight="1" 
    			android:layout_margin="4dip"
    			android:gravity="center" android:id="@+id/button8">
    		</Button>
    		<Button
    			android:background="@drawable/rounded_button1" android:typeface="monospace" 
    			android:text="9"
    			android:layout_width="0dip" 
    			android:textSize="70dp"
    			android:layout_height="100dip" 
    			android:padding="10dip" 
    			android:layout_weight="1" 
    			android:layout_margin="4dip"
    			android:gravity="center" android:id="@+id/button9">
    		</Button>
    </TableRow>
    <TableRow android:id="@+id/tableRow3" android:layout_width="wrap_content" android:layout_height="wrap_content">
        <Button
    			android:background="@drawable/rounded_button1" android:typeface="monospace" 
    			android:text="4"
    			android:layout_width="0dip" 
    			android:layout_height="100dip" 
    			android:padding="10dip" 
    			android:textSize="70dp"
    			android:layout_weight="1" 
    			android:layout_margin="4dip"
    			android:gravity="center" android:id="@+id/button4">
    		</Button>
    		<Button
    			android:background="@drawable/rounded_button1" android:typeface="monospace" 
    		    android:text="5"
    			android:layout_width="0dip" 
    			android:layout_height="100dip" 
    			android:textSize="70dp"
    			android:padding="10dip" 
    			android:layout_weight="1" 
    			android:layout_margin="4dip"
    			android:gravity="center" android:id="@+id/button5">
    		</Button>
    		<Button
    			android:background="@drawable/rounded_button1" android:typeface="monospace" 
    			android:text="6"
    			android:layout_width="0dip" 
    			android:layout_height="100dip" 
    			android:textSize="70dp"
    			android:padding="10dip" 
    			android:layout_weight="1" 
    			android:layout_margin="4dip"
    			android:gravity="center" android:id="@+id/button6">
    		</Button>
    </TableRow>
    <TableRow android:id="@+id/tableRow4">
        <Button
    			android:background="@drawable/rounded_button1" android:typeface="monospace" 
    			android:text="1"
    			android:layout_width="0dip" 
    			android:layout_height="100dip" 
    			android:textSize="70dp"
    			android:padding="10dip" 
    			android:layout_weight="1" 
    			android:layout_margin="4dip"
    			android:gravity="center" android:id="@+id/button1">
    		</Button>
    		<Button
    			android:background="@drawable/rounded_button1" android:typeface="monospace" 
    		    android:text="2"
    			android:layout_width="0dip" 
    			android:layout_height="100dip" 
    			android:textSize="70dp"
    			android:padding="10dip" 
    			android:layout_weight="1" 
    			android:layout_margin="4dip"
    			android:gravity="center" android:id="@+id/button2">
    		</Button>
    		<Button
    			android:background="@drawable/rounded_button1" android:typeface="monospace" 
    			android:text="3"
    			android:layout_width="0dip" 
    			android:layout_height="100dip" 
    			android:textSize="70dp"
    			android:padding="10dip" 
    			android:layout_weight="1" 
    			android:layout_margin="4dip"
    			android:gravity="center" android:id="@+id/button3">
    		</Button>
    </TableRow>
</TableLayout>
